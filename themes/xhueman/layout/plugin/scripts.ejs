<% if (typeof(isHead) !== 'undefined' && isHead) { %>
    <% if (theme.plugins.lightgallery) { %>
        <%- cdn('vendor/lightgallery/css/lightgallery.min.css') %>
    <% } %>
    <% if (theme.plugins.google_analytics) { %>
        <%- partial('plugin/google-analytics') %>
    <% } %>
    <% if (theme.plugins.baidu_analytics) { %>
        <%- partial('plugin/baidu-analytics') %>
    <% } %>
    <% if (theme.plugins.piwik_analytics) { %>
        <%- partial('plugin/piwik-analytics') %>
    <% } %>
<% } else { %>
    <% if (theme.plugins.lightgallery) { %>
        <%- cdn('vendor/lightgallery/js/lightgallery.min.js') %>
        <%- cdn('vendor/lightgallery/js/lg-thumbnail.min.js') %>
        <%- cdn('vendor/lightgallery/js/lg-pager.min.js') %>
        <%- cdn('vendor/lightgallery/js/lg-autoplay.min.js') %>
        <%- cdn('vendor/lightgallery/js/lg-fullscreen.min.js') %>
        <%- cdn('vendor/lightgallery/js/lg-zoom.min.js') %>
        <%- cdn('vendor/lightgallery/js/lg-hash.min.js') %>
        <%- cdn('vendor/lightgallery/js/lg-share.min.js') %>
        <%- cdn('vendor/lightgallery/js/lg-video.min.js') %>
    <% } %>
<% } %>

